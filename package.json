{
	"name": "dogear",
	"description": "A hapi plugin for sending expanded metrics to statsd-compliant services",
	"version": "2.0.3",
	"engines": {
		"node": ">=8"
	},
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint src",
		"test": "tap --cov --reporter=spec test/*.test.js",
		"precoverage": "rimraf coverage",
		"coverage": "tap --coverage-report=lcov"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/zefferus/dogear"
	},
	"keywords": [
		"hapi",
		"plugin",
		"statsd",
		"graphite",
		"metrics",
		"instrumentation",
		"stats",
		"request",
		"datadog",
		"dogstatsd",
		"influxdb",
		"telegraf"
	],
	"author": "Brandon Belvin",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/zefferus/dogear/issues"
	},
	"homepage": "https://github.com/zefferus/dogear#readme",
	"dependencies": {
		"hoek": "^5.0.3",
		"hot-shots": "^5.0.1",
		"lodash.at": "^4.6.0",
		"oppsy": "^2.0.0"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.1",
		"eslint": "^4.17.0",
		"hapi": "^17.2.0",
		"rimraf": "^2.6.2",
		"sinon": "^4.2.2",
		"tap": "^11.1.0"
	},
	"peerDependencies": {
		"hapi": ">=17"
	}
